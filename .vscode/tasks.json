{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "startLiveServer",
			"type": "process",
			"command": "${input:startServer}"
		},
		{
			"label": "stopLiveServer",
			"type": "process",
			"command": "${input:stopServer}"
		}
	],
	"inputs": [
		{
			"id": "startServer",
			"type": "command",
			"command": "extension.liveServer.goOnline"
		},
		{
			"id": "stopServer",
			"type": "command",
			"command": "extension.liveServer.goOffline"
		}
	]
}
